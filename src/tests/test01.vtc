varnishtest "Test maxmind_geoIP vmod"

server s1 {
       rxreq
       expect req.http.X-Request-Start ~ "t=[0-9]+" 
       txresp
} -start

varnish v1 -vcl+backend {
	import maxmind_geoIP from "${vmod_topbuild}/src/.libs/libvmod_maxmind_geoIP.so";

	sub vcl_recv {
		maxmind_geoIP.set_x_request_start();
	}

} -start

client c1 {
	txreq -url "/"
	rxresp
}

client c1 -run

